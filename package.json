    "@types/domready": "^1.0.0",
    "@types/ejs": "^2.7.0",
    "@types/glob": "^7.1.2",
    "@types/graceful-fs": "^4.1.3",
    "@types/gulp": "^3.8.36",
    "@types/gulp-help": "^3.0.27-alpha",
    "@types/gulp-util": "^3.0.35",
    "@types/js-beautify": "^1.11.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node-sass": "^3.10.32",
    "@types/ramda": "^0.27.6",
    "@types/request": "^2.48.5",
    "@types/rimraf": "^3.0.0",
    "@types/sizzle": "^2.3.2",
    "@types/webpack": "^4.41.18",
    "@types/yargs": "^15.0.5",
    "babel-jest": "^26.1.0",
